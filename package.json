{
  "name": "frontend-projects",
  "version": "1.0.0",
  "description": "Tool for developing Front-end applications written using HTML/EJS, CSS/SASS/SCSS and JavaScript",
  "source": "src/index.html",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "start": "parcel ./src/index.html --dist-dir ./docs",
    "build": "parcel build ./src/index.html --dist-dir ./docs --public-url ./",
    "reset": "node reset.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shariult/frontend-projects.git"
  },
  "author": "Md. Shariul Islam",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shariult/frontend-projects/issues"
  },
  "homepage": "https://github.com/shariult/frontend-projects#readme",
  "parcel-namer-struct": [
    {
      "type": [
        "css"
      ],
      "dest": "/css/"
    },
    {
      "type": [
        "ttf",
        "woff",
        "woff2",
        "eot",
        "otf"
      ],
      "dest": "/css/webfonts/"
    },
    {
      "type": [
        "js"
      ],
      "dest": "/js/"
    },
    {
      "type": [
        "jpg",
        "png",
        "jpeg",
        "gif",
        "svg",
        "mp4",
        "webm"
      ],
      "dest": "/img/"
    }
  ],
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@parcel/packager-raw-url": "^2.8.3",
    "@parcel/transformer-sass": "^2.8.3",
    "@parcel/transformer-webmanifest": "^2.8.3",
    "autoprefixer": "^10.4.14",
    "parcel": "^2.8.3",
    "parcel-namer-struct": "^1.0.0",
    "parcel-reporter-clean-dist": "^1.0.4",
    "parcel-transformer-ejs": "^1.0.1",
    "postcss": "^8.4.23",
    "prettier": "^2.8.7",
    "pug": "^3.0.2",
    "sass": "^1.60.0",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.7",
    "axios": "^1.3.4",
    "bootstrap": "^5.2.3",
    "bulma": "^0.9.4",
    "d3": "^7.8.4",
    "jquery": "^3.6.4",
    "tailwindcss": "^3.3.1"
  }
}
